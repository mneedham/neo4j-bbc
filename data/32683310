<script type="text/javascript">

    var chartbeatAssetType = 'story';
    var chartbeatSection = 'Football/Champions League'.replace(/\/.+/,'').replace(' ', '-').toLowerCase();;
    var chartbeatSections = ['sport', chartbeatSection];
     
    chartbeatSections.push('story');
    
    var chartbeatStoryTitle = "BBC+Sport+-+Bayern+Munich+3-2+Barcelona+%28agg+3-5%29".replace(new RegExp('\\+', 'gi'),' ');//remove the plus signs
    chartbeatStoryTitle = decodeURIComponent(chartbeatStoryTitle.replace("BBC Sport - ","")) + ' - BBC Sport';

    var _sf_async_config={};
    /** CONFIGURATION START **/
    _sf_async_config.uid = 50924; 
    _sf_async_config.domain = 'bbc.co.uk';
    _sf_async_config.sections = chartbeatSections.toString();
    _sf_async_config.region = 'domestic';
    _sf_async_config.path = 'bbc.co.uk/sport/football/32683310';
    _sf_async_config.title = chartbeatStoryTitle;

    (function() {
        var noCookies = true;
        var cookiePrefix = '_chartbeat';
        if ("object" === typeof bbccookies && typeof bbccookies.readPolicy == 'function') {
          noCookies = !bbccookies.readPolicy().performance;
        }
        if (noCookies && document.cookie.indexOf(cookiePrefix) !== -1) {
          var cookieSplit = document.cookie.split(';');
          var cookieLength = cookieSplit.length;
          while (cookieLength--) {
            var cookie = cookieSplit[cookieLength].replace(/^\s+|\s+$/g, '');
            var cookieName = cookie.split('=')[0];
            
            if (cookieName.indexOf(cookiePrefix) === 0) {
              document.cookie = cookieName + '=; expires=Thu, 01 Jan 1970 00:00:00 GMT; path=/;';
            }
          }
        }
        _sf_async_config.noCookies = noCookies;
    })();
    /** CONFIGURATION END **/
    (function(){
      function loadChartbeat() {
        window._sf_endpt=(new Date()).getTime();
        var e = document.createElement("script");
        e.setAttribute("language", "javascript");
        e.setAttribute("type", "text/javascript");
        e.setAttribute('src', '//static.chartbeat.com/js/chartbeat.js');
        document.body.appendChild(e);
      }
      var oldonload = window.onload;
      window.onload = (typeof window.onload != "function") ?
         loadChartbeat : function() { oldonload(); loadChartbeat(); };
    })();
</script>




